{
	"name": "frontend-monorepo",
	"version": "1.0.0",
	"private": true,
	"description": "Frontend monorepo project",
	"scripts": {
		"dev:admin": "pnpm --filter administrator dev",
		"build:admin": "pnpm --filter administrator build",
		"start:admin": "pnpm --filter administrator start",
		"dev:mts-gpon": "pnpm --filter mts-gpon dev --port 3001",
		"build:mts-gpon": "pnpm --filter mts-gpon build",
		"start:mts-gpon": "pnpm --filter mts-gpon start",
		"build:common": "pnpm --filter common build",
		"lint": "pnpm -r lint",
		"clean": "pnpm -r exec -- rimraf node_modules dist .next",
		"install:all": "pnpm install"
	},
	"devDependencies": {
		"rimraf": "^5.0.5"
	},
	"packageManager": "pnpm@10.29.3"
}
